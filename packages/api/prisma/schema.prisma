generator client {
  provider = "prisma-client-js"
  output   = "./generated-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Tweet {
  id        String   @id
  user      String
  createdAt DateTime
  text      String
  source    String
  retweets  Int
  likes     Int
}
